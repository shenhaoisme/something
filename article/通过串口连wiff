wpa_cli;
set_network ALE-TPC;
set_network 1 key_mgmt WPA-PSK;
set_network 1 psk "hxfx@2015"

wpa_supplicant -B -i wlan0 -c <(wpa_passphrase "ALE-TPC" "hxfx@2015")
wpa_passphrase "ALE-TPC" "hxfx@2015" > ./wpa.conf
wpa_supplicant -i wlan0 -B -c  ./wpa.conf
